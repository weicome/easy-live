{
    "name": "easyswoole/utility",
    "description": "easyswoole extension tool library",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "evalor",
            "email": "mipone@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "ext-openssl": "*",
